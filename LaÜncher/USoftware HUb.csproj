<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <UseWindowsForms>False</UseWindowsForms>
    <AssemblyName>HUBCore</AssemblyName>
    <ApplicationIcon>LOGO_U.ico</ApplicationIcon>
    <ProduceReferenceAssembly>False</ProduceReferenceAssembly>
	<GenerateAssemblyInfo>True</GenerateAssemblyInfo>
	<NuGetAuditMode>all</NuGetAuditMode>
	<NuGetAuditLevel>moderate</NuGetAuditLevel>
	<Company>ÜSoftware</Company>
	<Authors>Karinko Kürumane</Authors>
	<Copyright>Copyright © ÜSoftware 2025</Copyright>
	<Product>ÜSoftware HÜB</Product>
	<PackageIcon>LOGO_U.png</PackageIcon>
	<Version>1.0.0</Version>
	<FileVersion></FileVersion>
	<RepositoryUrl>https://github.com/KarinokoKurumane/USoftware-HUb-DEMO</RepositoryUrl>
	<PackageProjectUrl>https://karinokokurumane.github.io/USoftware-HUb-DEMO</PackageProjectUrl>
	<PackageTags>C#;Gaming;Shop;Applications;WPF;</PackageTags>
	<NeutralLanguage>pl-PL</NeutralLanguage>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DebugType>none</DebugType>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DebugType>none</DebugType>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\assistance_banner.png" />
