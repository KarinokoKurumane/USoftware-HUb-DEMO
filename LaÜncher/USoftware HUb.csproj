    <None Remove="Assets\discord_logo.png" />
    <None Remove="Assets\factorio.png" />
    <None Remove="Assets\github_logo.png" />
    <None Remove="Assets\gmail_logo.png" />
    <None Remove="Assets\LOGO_U.ico" />
    <None Remove="Assets\LOGO_U.png" />
    <None Remove="Assets\name_changer_banner.png" />
    <None Remove="Assets\name_changer_ico.png" />
    <None Remove="Assets\nasterydachlogo.png" />
    <None Remove="Assets\satisfactory.png" />
    <None Remove="Assets\SmallLogo.png" />
    <None Remove="Commisions\faq.json" />
    <None Remove="ProductsData\apps.json" />
    <None Remove="ProductsData\games.json" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\assistance_ico.png" />
    <Resource Include="Assets\name_changer_banner.png" />
    <Resource Include="Assets\name_changer_ico.png" />
