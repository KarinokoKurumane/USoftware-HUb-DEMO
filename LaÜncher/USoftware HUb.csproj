  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\assistance_banner.png" />
    <Resource Include="Assets\discord_logo.png" />
    <Resource Include="Assets\factorio.png">
      <CopyToOutputDirectory></CopyToOutputDirectory>
    </Resource>
    <Resource Include="Assets\github_logo.png" />
    <Resource Include="Assets\gmail_logo.png" />
